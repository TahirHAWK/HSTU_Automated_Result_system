<html lang="en">
    <%- include('./partials/head.ejs') %>

<body>
    <%- include('./partials/navbar.ejs') %>

  
  <div class="main">


    
    <h2>Admin Dashboard</h2>
    <h3>Welcome <%= registerName %> </h3>

    <% for (i = 1; i <= 4; i ++){ %>
      <% for(j = 1; j <= 2; j++ ){ %>
    <!-- <label for="card_holder">Level <%= i %> Semester <%= j %></label> -->
    <label for="card_holder" class="cardHold"><%= 'Level '+  i +  ' Semester '+  j  %></label>
    <div class="card_holder">
        <% courseData.forEach((course) => { %>
          <% if(course.levelSemester == ('level '+  i +  ' semester '+  j)) {%>
        <div class="card text-center">
        <div class="card-header"><%= course.course_code %></div>
        <div class="card-body">
          <h5 class="card-title"><%= course.course_title %></h5>
          <p class="card-text">
            Degree: <%= course.degree %> <br> <%= course.levelSemester %> <br> credits: <%= course.credit %> <br> 
          </p>
          <a href="/admin/courses/<%=  course.course_code %>" class="btn btn-primary">Assign Teacher</a>
        </div>
        <% if(course.assignedTeacher){ %>
        <div class="card-footer text-muted">Assigned Teacher: <%= course.assignedTeacher %></div>
      </div>
       <% } else { %>
        <div class="card-footer text-muted">Assigned Teacher: none</div>
      </div>
        <% } %>
      <% } %>
 
      <% }) %>
    </div>
    <% }} %>

      

    
    
    
    
  </div>
    
  </div>
</body>
</html>